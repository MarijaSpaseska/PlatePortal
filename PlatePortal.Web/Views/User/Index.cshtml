@model List<PlatePortal.Domain.Identity.PlatePortalApplicationUser>

<div class="text-center">
    <h1>Import Users</h1>
    <hr />
</div>

<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-6 m-auto">
            <form method="post" asp-controller="User" asp-action="ImportUsers" enctype="multipart/form-data">
                <div class="form-group">
                    <input type="file" name="file" class="form-control" />
                </div>
                <div class="form-group text-center">
                    <div class="col-sm-12 btn-submit">
                        <button type="submit" class="btn btn-success">Import Excel File</button>
                    </div>
                </div>
            </form>
        </div>
    </div>

</div>

